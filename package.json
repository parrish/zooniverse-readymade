{
  "name": "zooniverse-readymade",
  "version": "0.0.1",
  "dependencies": {
    "coffee-script": "~1.7.1",
    "haw": "~0.3.8",
    "jquery": "~2.1.0",
    "marking-surface": "~0.5.2",
    "publisssh": "~0.2.4",
    "stack-of-pages": "~0.1.3",
    "tosource": "~0.1.1",
    "zooniverse": "~0.3.8"
  },
  "scripts": {
    "start": "haw serve --config ./app/haw-config --project ./example/project",
    "stage": "npm run build && publisssh ./build demo.zooniverse.org/readymade && npm run cleanup",

    "build": "haw build ./build --config ./app/haw-config --project ./example/project",
    "cleanup": "rm -rf ./build"
  }
}
